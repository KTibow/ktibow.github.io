---
import BlogPost from '$lib/layouts/BlogPost.astro';
import dock from './dock.png';
import roomba from './roomba.png';
import dockIr from './dock ir.png';
import buoy from './1 buoy.png';

export const title = 'Roomba dock IR, illustrated';
export const date = '2025-10-25';
---

<BlogPost {date} {title}>
  <img {...dock} alt="Roomba dock" />
  <img {...roomba} alt="Roomba" />
  <img {...dockIr} alt="Roomba dock IR" />
  <img {...buoy} alt="Roomba dock buoy" />
</BlogPost>
