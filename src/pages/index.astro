</Base>

<style>
  article {
    margin: 1.5rem;
  }
  .project-header {
    @apply --m3-headline-large;
    margin: 1.5rem;
  }
  .projects {
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
    margin-inline: 1.5rem;
  }
  .project {
    display: flex;
    flex-direction: column;

    flex: 1;
    flex-basis: 15rem;
    max-width: 30rem;
    border-radius: 1rem;
    background-color: var(--m3c-surface-container);
    > img {
      border-radius: inherit;
      margin-bottom: auto;
      aspect-ratio: 2 / 1;
      object-fit: contain;
    }
    > p,
    > time {
      margin-inline: 0.5rem;
    }
    > p {
      margin-block: 0.5rem 0;
    }
    > time {
      margin-block: 0.5rem;
    }
  }
  time {
    opacity: 0.8;
  }
</style>
